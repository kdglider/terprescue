<launch>
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find terprescue)/worlds/darpa.world"/>
    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="true"/>
    <arg name="headless" value="false"/>
    <arg name="debug" value="false"/>
  </include>

  <arg name="ns"     default="turtlebot"/>
  <include file="$(find terprescue)/launch/includes/turtlebot3_base.launch">
    <arg name="model" value="waffle"/>
    <arg name="ns"    value="$(arg ns)"/>
    <arg name="y"     value="-6.0"/>
    <arg name="z"     value="0.5"/>
  </include>

  <node type="rviz" name="rviz1" pkg="rviz" args="-d $(find terprescue)/rviz/rviz.rviz" />

</launch>
